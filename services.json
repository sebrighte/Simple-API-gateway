{
    "services": [
        {
            "name": "httpbin",
            "endpoint": "https://httpbin.org/",
            "swagger": "https://httpbin.org/spec.json",
            "gateway_api": {
                "name": "gateway_key",
                "in": "query",
                "required": true,
                "schema": {
                    "type": "string",
                    "default": "apikey123"
                },
                "description": "Gateway API key"
            }
        }
    ]
}